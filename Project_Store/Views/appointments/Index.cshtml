@model IEnumerable<Project_Store.Models.appointment>

@{
    ViewBag.Title = "Index";
}

<h2>Citas</h2>

<p>
    <a class="black-text" href='@Url.Action("Create", "appointments")'><i class="material-icons icon medium">event</i></a>
</p>
<table class="table">
    <tr>
        <th>
           Fecha Cita
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.date)
    </td>
 
    <td>
        <a class="black-text" href='@Url.Action("Edit", "appointments", new { id = item.id })'><i class="material-icons">edit</i></a>
    </td>
    <td>|</td>
    <td>
        <a class="black-text" href='@Url.Action("Delete", "appointments", new { id = item.id })'><i class="material-icons">delete</i></a>
    </td>
</tr>
}

</table>
